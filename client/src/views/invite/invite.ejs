---
group: invite
state:
    default: 기본
---
<!DOCTYPE html>
<head>
    <%- include(`${process.env.INCLUDE_PATH}/components/_head`, {
        title: '은&지우 결혼식 모바일 청첩장',
        desc: '✨EUN & JIWOO wedding ceremony invite'
    }); %>
    <link rel="stylesheet" href="<%= process.env.BASE_PATH %>/styles/invite/styles<%= process.env.MINIFY %>.css<%= `?${setTimestamp()}`; %>">
</head>
<body>
    <%- include(`${process.env.INCLUDE_PATH}/invite/partials/_header`); %>
    <main role="main">
        <% for(let i = 0; i < 5; i++) { %>
            <%- include(`${process.env.INCLUDE_PATH}/invite/partials/_contents`, {index: i}); %>
        <% } %>
        <div class="backgrounds">
            <% for (let i = 100; i--;) { %>
                <span class="background">
                    <span class="circle"></span>
                </span>
            <% } %>
        </div>
    </main>
    <%- include(`${process.env.INCLUDE_PATH}/invite/partials/_footer`); %>
    <script src="https://oapi.map.naver.com/openapi/v3/maps.js?ncpClientId=hd2fu3fh50&submodules=geocoder"></script>
    <script src="<%= process.env.BASE_PATH; %>/scripts/invite/scripts<%= process.env.MINIFY %>.js<%= `?${setTimestamp()}`; %>" defer></script>
</body>